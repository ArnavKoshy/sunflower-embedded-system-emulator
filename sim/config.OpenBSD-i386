CC			= gcc
GAWK			= gawk
LINT			= echo 
LD			= ld
BISON			= bison
ENDIAN			= SF_L_ENDIAN
PLATFORM_CFLAGS 	= -pthread -D$(ENDIAN)
PLATFORM_LFLAGS 	= -pthread 
PLATFORM_OPTFLAGS	=
OPTFLAGS		= -O0
DBGFLAGS		= -g
CCFLAGS			= $(DBGFLAGS) $(PLATFORM_CFLAGS) $(PLATFORM_OPTFLAGS)
LDFLAGS 		= -lm $(PLATFORM_LFLAGS)
TARGET-ARCH-FLAGS	= -DM32
